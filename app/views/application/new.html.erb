<%= form_with url: "/applications", method: :post, local: true do |f| %>
  <%= f.label          :name %>
  <%= f.text_field     :name %><br>
  <%= f.label          :street %>
  <%= f.text_field     :street %><br>
  <%= f.label          :city %>
  <%= f.text_field     :city %><br>
  <%= f.label          :state %>
  <%= f.text_field     :state %><br>
  <%= f.label          :zipcode %>
  <%= f.text_field     :zipcode %><br>
  <%= f.submit 'Submit Application' %>
<% end %>
